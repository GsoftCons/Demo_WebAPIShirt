@model List<Shirt>

<h3>Shirt</h3>
    <br/>
    <table class="table table-striped">
        <thead>
            <tr>
                <td>Brand</td>
                <td>Gender</td>
                <td>Color</td>
                <td>Size</td>
                <td>Prize</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var shirt in Model)
            {
                <tr>
                <td>@shirt.ShirtId</td>
                    <td>@shirt.Brand</td>
                    <td>@shirt.Gender</td>
                    <td>@shirt.Color</td>
                    <td>@shirt.Size</td>
                    <td>@(shirt.Price.ToString("c"))</td>
                </tr>
        }
        </tbody>
    </table>